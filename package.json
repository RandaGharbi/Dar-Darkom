{
  "name": "nourane",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "apps/web/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check --continue",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "backend:network": "cd apps/backend && BASE_URL=http://192.168.1.73:5000 HOST=0.0.0.0 npm run dev",
    "backend:local": "cd apps/backend && npm run dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/express": "^4.17.0",
    "@types/multer": "^1.4.7",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "prettier": "^3.0.0",
    "turbo": "^2.5.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "expo-router": "^6.0.4"
  }
}
