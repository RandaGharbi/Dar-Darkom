{
  "expo": {
      "name": "Guerlain",
  "slug": "Guerlain",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoGuerlain.png",
    "scheme": "guerlain",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rindaa.Guerlain",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.rindaa.Guerlain",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoGuerlain.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/logoGuerlain.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoGuerlain.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ff626a90-3b85-4261-aae5-27538bf66006"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/ff626a90-3b85-4261-aae5-27538bf66006"
    }
  }
}
